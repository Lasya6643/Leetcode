class Solution {
    public void moveZeroes(int[] a) {
        int i=0;
        int n=a.length;
        while(i<n &&a[i]!=0){
            i++;
            }
        for(int j=i+1;j<n;j++){
            if(a[j]!=0){
              a[i]=a[i]^a[j];
              a[j]=a[i]^a[j];
              a[i]=a[i]^a[j];
              i++;
            }
        }
    }
